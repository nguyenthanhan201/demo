{
  "extends": "@repo/typescript-config/nextjs.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next",
      },
    ],
    "baseUrl": ".",
    "paths": {
      "*": ["./@mf-types/*"],
    },
  },
  "include": [
    "next-env.d.ts",
    "next.config.js",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "*.d.ts",
  ],
  "exclude": ["node_modules"],
}
